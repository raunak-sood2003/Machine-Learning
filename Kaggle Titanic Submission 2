#Imports
import pandas as pd
from sklearn.tree import DecisionTreeClassifier
from sklearn.model_selection import train_test_split
import numpy as np
from sklearn import metrics
#Reading Files
train = 'C:\\Users\\rrsoo\\Advanced Stuff\\Titanic\\train.csv'
test = "C:\\Users\\rrsoo\\Advanced Stuff\\Titanic\\test.csv"
gender_submission = "C:\\Users\\rrsoo\\Advanced Stuff\\Titanic\\gender_submission.csv"
read_training_data = pd.read_csv(train)
read_testing_data = pd.read_csv(test)
read_submission_data = pd.read_csv(gender_submission)
read_training_data.columns
#Training and Testing Data
features = ['Age', 'SibSp', 'Parch', "Sex", "Fare", "Pclass", "Embarked"]
X_train = read_training_data[features]
y_train = read_training_data.Survived
X_train = X_train.fillna(0) #Filling Nans to 0
X_train = X_train.replace("male", 1) #Replacing strings with numbers
X_train = X_train.replace("S",0)
X_train = X_train.replace("Q",1)
X_train = X_train.replace("C",2)
y_train = y_train.fillna(0)
X_train = X_train.replace("female", 0)
X_test = read_testing_data[features]
X_test = X_test.fillna(0)
X_test = X_test.replace("female", 0)
X_test = X_test.replace("male", 1)
X_test = X_test.replace("S",0)
X_test = X_test.replace("Q",1)
X_test = X_test.replace("C",2)
#Fitting
model = DecisionTreeClassifier(random_state = 4)
model.fit(X_train, y_train)
#Predicting
y_preds = model.predict(X_test)
model.predict(X_test)
#68.1% Accuracy
